#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/petercvek/Projects/Influee/remotion/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/petercvek/Projects/Influee/remotion/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-jest@27.0.5_y7dikhajzvqh55tyzs44tmi6b4/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-jest@27.0.5_y7dikhajzvqh55tyzs44tmi6b4/node_modules/ts-jest/cli.js" "$@"
fi
